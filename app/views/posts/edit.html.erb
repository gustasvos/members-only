<%= form_with model: @post do |form|%>
    <div class="col-md-3 mb-3 mx-auto">
        <%= form.label "Edit your post", :class => "form-label" %>
        <%= form.text_field :body, :class => "form-control"%> 
        <%= form.submit "Edit your post", :class => "btn btn-primary mt-2"%>
    </div>
<% end %>